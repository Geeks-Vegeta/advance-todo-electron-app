  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^7.4.0",
    "crypto-browserify": "^3.12.0",
    "electron-is-dev": "^2.0.0",
    "fs": "0.0.1-security",
    "mdb-react-ui-kit": "^4.2.0",
    "moment": "^2.29.4",
    "nedb": "^1.8.0",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-datetime-picker": "^3.5.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "sqlite3": "^5.0.11",
    "stream-browserify": "^3.0.0",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "react-start": "SET BROWSER=none && react-scripts start",
    "react-build": "react-scripts build",
    "start": "concurrently \"SET BROWSER=none && npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build",
    "rebuild_sqlite": "electron-rebuild -f -w sqlite3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browser": {
    "fs": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^20.1.3",
    "electron-rebuild": "^3.2.9"
  }
}
